{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prefer-stateless-function": [0],
        "react/jsx-indent": "off",
        "react/jsx-indent-props": "off",
        "react/jsx-curly-brace-presence": "off",
        "no-param-reassign": "off",
        "indent": "off"
    }
}
